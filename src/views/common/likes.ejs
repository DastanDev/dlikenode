                        <h4 class="post-title single_title"><a href="/post/<%- like.author %>/<%- link %></a></h4>
                        <p class="post-entry post-tags"><%-  %></p>
                        <div class="post-comments bottom_block">
                        <div><img src="/images/dlike-hover.png" alt="DLIKE" class="hov_vote" data-permlink="<%-  %>" data-author="<%-  %>"> | <span id="post_likes" class="post_likes"><%-  %></span>LIKES</div>
                <div><span class="dlike_tokens"><%-  %></span> <b>DLIKE</b>
                </div></div>
        </article>
    </div><% }) %>
